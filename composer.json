{
    "name": "wethered/docker-api",
    "description": "",
    "keywords": [
        "wethered",
        "docker-api"
    ],
    "homepage": "https://github.com/wethered/docker-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Kadir Yamamoto",
            "email": "me@yamakadi.com",
            "homepage": "https://wethe.red",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "jane-php/open-api-runtime": "~6.1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "jane-php/open-api-2": "~6.1.0",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^3.11"
    },
    "autoload": {
        "psr-4": {
            "WeTheRed\\DockerApi\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WeTheRed\\DockerApi\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
